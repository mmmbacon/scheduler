{
  "env": {
    "browser": true,
    "jest": true,
    "cypress/globals": true
  },
  "extends": [
    "react-app",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "react",
    "cypress"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "babel-eslint",
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}